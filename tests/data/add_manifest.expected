      "name": "add.manifest",
      "authorization": [{
          "actor": "dapp1",
          "permission": "active"
        }
      ],
      "data": {
        "account": "dapp1",
        "name": "distributed app 1",
        "domain": "https://nowhere",
        "icon": "0123456789ABCDEF0123456789ABCDEF0123456789ABCDEF0123456789ABCDEF",

        "whitelist": [{
            "contract": "contract.2",
            "action": ""
          },{
            "contract": "contract.1",
            "action": "just.this"
          },{
            "contract": "contract.3",
            "action": ""
          }
        ],
        "blacklist": [{
            "contract": "contract.3",
            "action": "bad"
          },{
            "contract": "contract.2",
            "action": "very.bad"
